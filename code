:root {
    --NPF-Caption-Spacing:1em;
    --NPF-Image-Spacing:4px;
    
    /*---------- NIGHT MODE COLOR CUSTOMIZATION (DO NOT TOUCH)----------*/
    
    --accent-night: {color:accent night};
    --accented-text-night: {color:accented text night};
    --background-night: {color:background night};
    --lightbox-background-night: rgba({RGBcolor:background night}, 0.6);
    --text-night: {color:text night};
    --bold-night: {color:bold night};
    --italic-night: {color:italic night};
    --links-night: {color:links night};
    --link-hover-night: {color:link hover night};
    --posts-background-night: {color:posts background night};
    --night-borders: {color:night borders};
    --posts-title-night: {color:posts title night};
    --sidebar-title-night: {color:sidebar title night};
    --navigation-night: {color:navigation night};
    --navigation-hover-night: {color:navigation hover night};
